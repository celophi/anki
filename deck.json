            "css": "@font-face {\n  font-family: 'Noto Sans JP';\n  src: url('https://fonts.googleapis.com/css2?family=Noto+Sans+JP:wght@400;700&display=swap');\n}\n\n/* Minimalist and elegant Anki card style */\n.card-container {\n  font-family: 'Noto Sans JP', Arial, sans-serif;\n  background: #1e1e2e;\n  color: #cdd6f4;\n  padding: 8px;\n  margin: 10px auto 0;\n  text-align: left;\n  max-width: 700px;\n}\n\n.card-header {\n  background: transparent;\n  border-radius: 12px;\n  color: inherit;\n  padding: 0;\n  box-shadow: none;\n}\n\n.card-title {\n  font-size: 2em;\n  font-weight: bold;\n  color: #89b4fa;\n  margin-bottom: 16px;\n  text-align: center;\n}\n\n.divider {\n  border: none;\n  border-top: 1px solid #313244;\n  margin: 16px 0;\n}\n\n.card-content {\n  display: flex;\n  flex-direction: column;\n  gap: 16px;\n}\n\n.card-section {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  padding: 12px;\n  background: #313244;\n  border-radius: 8px;\n  font-size: 1.2em;\n}\n\n.section-label {\n  font-size: 1em;\n  font-weight: 600;\n  color: #cdd6f4;\n  min-width: 15%;\n}\n\n.reading {\n  color: #a6e3a1;\n  font-weight: 400;\n  border-left: 3px solid #a6e3a1;\n  padding-left: 12px;\n}\n\n.english-definition {\n  color: #e78c45;\n  font-weight: 400;\n  border-left: 3px solid #e78c45;\n  padding-left: 12px;\n}\n\n.japanese-definition {\n  color: #f9e2af;\n  font-weight: 400;\n  border-left: 3px solid #f9e2af;\n  padding-left: 12px;\n}\n\n.example-sentence {\n  color: #f38ba8;\n  font-weight: 400;\n  border-left: 3px solid #f38ba8;\n  padding-left: 12px;\n}\n\n.translation {\n  color: #f5c2e7;\n  font-weight: 400;\n  border-left: 3px solid #f5c2e7;\n  padding-left: 12px;\n}\n\n.cloze-highlight {\n  color: #a6e3a1;\n  font-weight: 400;\n}\n\n@media (max-width: 500px) {\n  .card-container {\n    padding: 8px;\n    font-size: 1em;\n  }\n  .card-title {\n    font-size: 1.5em;\n  }\n  .card-section {\n    font-size: 1em;\n    padding: 8px;\n  }\n}",
